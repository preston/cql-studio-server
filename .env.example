# CQL Studio Server Environment Variables
# Copy this file to .env and configure as needed

# Server port (default: 3003)
CQL_STUDIO_SERVER_PORT=3003

# Environment mode (development/production)
CQL_STUDIO_SERVER_NODE_ENV=development

# Allowed CORS origin (default: http://localhost:4200)
CQL_STUDIO_SERVER_CORS_ORIGIN=http://localhost:4200

# Logging level (default: info)
CQL_STUDIO_SERVER_LOG_LEVEL=info
